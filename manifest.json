{
  "manifest_version": 3,
  "name": "OriginalUI",
  "version": "1.0.0",
  "description": "A Chrome extension built with React, Vite, and Tailwind CSS",
  "permissions": [
    "activeTab",
    "storage",
    "scripting",
    "alarms",
    "declarativeNetRequest",
    "declarativeNetRequestFeedback"
  ],
  "host_permissions": ["<all_urls>"],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "easylist-adservers",
        "enabled": true,
        "path": "network-blocking/data/rulesets/easylist-adservers.json"
      }
    ]
  },
  "background": {
    "service_worker": "scripts/background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["scripts/content.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "data/defaultRules.json",
        "data/defaultWhitelist.json",
        "network-blocking/data/default-block-requests.json",
        "scripts/injected-script.js",
        "index.css"
      ],
      "matches": ["<all_urls>"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "OriginalUI"
  },
  "icons": {
    "16": "icons/logo_16x16.png",
    "48": "icons/logo_48x48.png",
    "128": "icons/logo_128x128.png"
  }
}
